<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane id="border" fx:id="border" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.teamname.Controllers.Controller">
   <top>
      <ToolBar prefHeight="71.0" prefWidth="600.0" BorderPane.alignment="CENTER">
        <items>
          <Button id="homeButton" fx:id="homeButton" defaultButton="true" mnemonicParsing="false" onAction="#homeButtonClicked" text="Home" textAlignment="CENTER" />
            <Button id="featureOneButton" fx:id="featureOneButton" mnemonicParsing="false" onAction="#featureOneButtonClicked" text="Feature 1" textAlignment="CENTER" />
            <Button id="featureOneButton" fx:id="featureTwoButton" mnemonicParsing="false" onAction="#featureTwoButtonClicked" text="Feature 2" />
            <Separator orientation="VERTICAL" prefHeight="0.0" prefWidth="13.0" />
        </items>
      </ToolBar>
   </top>
   <bottom>
      <Button id="exitButton" fx:id="exitButton" alignment="TOP_CENTER" defaultButton="true" mnemonicParsing="false" onAction="#exitButtonClicked" text="Exit Application" textAlignment="RIGHT" BorderPane.alignment="TOP_CENTER">
         <BorderPane.margin>
            <Insets bottom="10.0" />
         </BorderPane.margin></Button>
   </bottom>
</BorderPane>
