<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ToolBar?>

<ToolBar id="toolbar" fx:id="toolbar" prefHeight="43.0" prefWidth="605.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.wpi.teamb.Controllers.Controller">
   <items>
      <Button id="homeButton" fx:id="homeButton" mnemonicParsing="false" onAction="#homeButtonClicked" text="Home" textAlignment="CENTER" />
      <Button id="featureOneButton" fx:id="featureOneButton" mnemonicParsing="false" onAction="#featureOneButtonClicked" text="Patient Transportation" textAlignment="CENTER" />
      <Button id="featureOneButton" fx:id="featureTwoButton" mnemonicParsing="false" onAction="#featureTwoButtonClicked" text="Sanitation Services" />
      <Button fx:id="databaseButton" mnemonicParsing="false" onAction="#databaseButtonClicked" text="Database" />
      <Button fx:id="pathfindingButton" mnemonicParsing="false" onAction="#pathfindingClicked" text="Pathfinding" />
      <MenuButton id="menuIcon" fx:id="menuButton" alignment="TOP_CENTER" mnemonicParsing="false" prefHeight="39.0" prefWidth="39.0">
         <items>
            <MenuItem mnemonicParsing="false" text="Help" />
            <MenuItem mnemonicParsing="false" text="Profile" />
            <MenuItem fx:id="exitButton" mnemonicParsing="false" onAction="#exitButtonClicked" text="Exit Application" />
         </items>
      </MenuButton>
   </items>
   <padding>
      <Insets left="30.0" />
   </padding>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</ToolBar>
